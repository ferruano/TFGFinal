<div id="conexiones" class="tab" style="display:none;" >
    <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
        <div class="carousel-inner">
            <div class="carousel-item active">
                <%if(process.env.TIPO=="alumno") {%>
                    <h2>UNIVERSIDADES</h2>
                    <img class="d-block w-25 mx-auto" src="https://img2.freepng.es/20181113/sty/kisspng-computer-icons-university-higher-education-school-government-svg-png-icon-free-download-2-6-76-5beba3fdc2c800.6793559415421695977978.jpg" alt="First slide">
                <%}%>
                <%if(process.env.TIPO=="universidad") {%>
                        <h2>ALUMNOS</h2>
                        <img class="d-block w-25 mx-auto" src="data:image/png;base64," alt="First slide">
                <%}%>
            </div>
            <%for(let relationship of relationships) {%>
                <div class="carousel-item">
                    <img class="d-block w-25 mx-auto" src="../public/images/iconoPersona.jpg">
                    <%if(relationship.metadata.name) {%>
                        <h3><%=relationship.metadata.name%></h3>
                    <%}%>
                    <fieldset>
                        <legend class="" style="border:none;margin:10px;">Relationship DIDs</legend>
                        <p>Mi DID: <%=relationship.my_did%></p>
                        <p>Su DID: <%=relationship.their_did%></p>
                    </fieldset>
                    <p>Their Public Did: <%=relationship.metadata.theirEndpointDid%></p>
                    <p><b>Proofs:</b></p>
                    <%if(relationship.metadata.proofs) {%>
                        <%for(let proof of relationship.metadata.proofs) {%>
                            <div class="w3-bar w3-card">
                                <div class="w3-bar-item">
                                    <p style="text-decoration: underline;"><%=proof.request.name%></p>
                                    <%for(let attr of Object.keys(proof.request.requested_attributes)) {%>
                                    <p><%=proof.request.requested_attributes[attr].name%>: <%=proof.requested_proof.revealed_attrs[attr].raw%></p>
                                    <%}%>
                                </div>
                                <div class="w3-bar-item">
                                    <button class="w3-btn w3-white w3-border" style="display:block;margin: 20px 20px;height: 100px;width:100px;" type="submit" onclick="validateProof(this.parentNode, <%=JSON.stringify(proof)%>)">Validar</button>
                                </div>
                            </div>
                        <br><br>
                        <%}%>
                    <%}%>
                </div>

            <%}%>
        </div>
        <a class="left carousel-control" href="#carouselExampleControls" data-slide="prev">
            <span class="glyphicon glyphicon-chevron-left"></span>
        </a>
        <a class="right carousel-control" href="#carouselExampleControls" data-slide="next">
            <span class="glyphicon glyphicon-chevron-right"></span>
        </a>
    </div>
    <div id="buttonAbajo">
        <button type="submit" class="btn btn-dark btn-lg btn-block" onclick="document.getElementById('newRelationshipModel').style.display='block'">Nueva Conexión</button>
    </div>

</div>